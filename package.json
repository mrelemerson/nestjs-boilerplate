    "@nestjs/core": "^8.4.4",
    "@nestjs/event-emitter": "^1.1.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.4",
    "@typegoose/typegoose": "^9.8.1",
    "aws-sdk": "^2.1134.0",
    "bcrypt": "^5.0.1",
    "big.js": "^6.1.1",
    "bull": "^4.8.3",
    "cache-manager": "^3.6.1",
    "cache-manager-redis-store": "^2.0.0",
    "change-case": "^4.1.2",
    "dot-object": "^2.1.4",
